<div class="container">
  <h2>The code is <code><%= @game.code %></code></h2>
  <p>Got to <code><%= join_url %></code> to join</p>
  <ul>
    <% @game.users.each do |user| %>
      <li><%= user.name %></li>
    <% end %>
  </ul>
  <%= link_to "Start game", game_round_path(@game.code, @game.rounds.first), class: "btn btn-primary" %>
</div>
